#!/usr/bin/perl
# 
# This takes a stock RHEL or CentOS 6.x ISO as the source for generating a Anvil! install ISO. 
# 
# It was created to avoid legal concerns regarding redistributing modified CentOS or RHEL ISOs. Please use it
# with this spirit in mind and do not make available the resulting ISOs for general download unless you get
# written permission from Alteeve's Niche! and either Red Hat or CentOS, depending on the generated ISO.
# 
# (c) Alteeve's Niche! Inc, 2015 - https://alteeve.ca
# 
# This program is released under the GPL v2+
# 

use strict;
use warnings;
use IO::Handle;
my $THIS_FILE =  $0;
   $THIS_FILE =~ s/^.*\///;

my $conf = {
	# Executable programs. We will search for these in PATH if not pre-defined (start with '/...').
	executable	=>	{
		# Used to verify that the source provided matches what we expect.
		md5sum		=>	"md5sum",
	},
	# Paths to files and directories
	path		=>	{
		# Source ISO will be mounted under here in a 'rhel' or 'centos' subdirectory, as appropriate.
		mount		=>	"/tmp/",
	},
	sys		=>	{
		debug		=>	0,
		md5sum		=>	{
			'a2bc9de743bfb0bfe1f14676a46a624b'	=>	"rhel",		# v6.7
			'51f90e8ddd2ea199780065eaf75130ad'	=>	"centos",	# v6.7
		},
		# This will be set to 'rhel' or 'centos', depending on the source ISO type.
		os		=>	"",
		source		=>	{
			directory	=>	"",
			file		=>	"",
		},
		use_anvil	=>	0,
	},
};

print "Beginning Anvil! ISO generation process. Please be patient!\n";
get_switches($conf);
$conf->{sys}{debug} = 1 if $conf->{switches}{debug};
if (($conf->{switches}{h})   || 
    ($conf->{switches}{'?'}) ||
    ($conf->{switches}{help}))
{
	print_usage($conf);
	exit(0);
}

find_executables($conf);

validate_source($conf);

# If we're still alive, we're ready.

get_package_list($conf);

if (1)
{
	print "# Common packages:\n";
	print "yum install ";
	foreach my $package (sort {$a cmp $b} @{$conf->{sys}{packages}{common}})
	{
		print "$package ";
	}
	print "\n\n";
}

if ($conf->{sys}{os} eq "rhel")
{
	print "# RHEL packages:\n";
	print "yum install ";
	foreach my $package (sort {$a cmp $b} @{$conf->{sys}{packages}{rhel}})
	{
		print "$package ";
	}
	print "\n\n";
}

if ($conf->{sys}{os} eq "centos")
{
	print "# CentOS packages:\n";
	print "yum install ";
	foreach my $package (sort {$a cmp $b} @{$conf->{sys}{packages}{centos}})
	{
		print "$package ";
	}
	print "\n\n";
}

if (1)
{
	print "# Alteeve packages:\n";
	print "yum install ";
	foreach my $package (sort {$a cmp $b} @{$conf->{sys}{packages}{alteeve}})
	{
		print "$package ";
	}
	print "\n\n";
}

if (1)
{
	print "# Third party, binary packages:\n";
	print "yum install ";
	foreach my $package (sort {$a cmp $b} @{$conf->{sys}{packages}{binary}})
	{
		print "$package ";
	}
	print "\n\n";
}

exit(0);

# This validates the source ISO. It will exit if problems are found.
sub validate_source
{
	my ($conf) = @_;
	
	if ($conf->{switches}{source})
	{
		if (-e $conf->{switches}{source})
		{
			print "[ Debug ] - Parsing source: [$conf->{switches}{source}]\n" if $conf->{sys}{debug};
			
			($conf->{sys}{source}{directory}, $conf->{sys}{source}{file}) = ($conf->{switches}{source} =~ /^(.*)\/(.*)$/);
			print "[ Debug ] - Source directory: [$conf->{sys}{source}{directory}], file: [$conf->{sys}{source}{file}]\n" if $conf->{sys}{debug};
		}
		else
		{
			print "\n[ Error ] - Source: [$conf->{switches}{source}] doesn't exist!\n\n";
			exit(1);
		}
		
		print "- Generating md5sum of the source, please wait a moment...\n";
		my $source_sum = "";
		my $shell_call = "$conf->{executable}{md5sum} $conf->{switches}{source}";
		print "[ Debug ] - shell_call: [$shell_call]\n" if $conf->{sys}{debug};
		open (my $file_handle, "$shell_call 2>&1 |") or die "\n[ Error ] - Failed to call: [$shell_call]. Received error: $!\n\n";
		while(<$file_handle>)
		{
			chomp;
			my $line = $_;
			print "[ Debug ] - line: [$line]\n" if $conf->{sys}{debug};
			
			if ($line =~ /^(\S+)\s+$conf->{switches}{source}/)
			{
				$source_sum = $1;
				print "[ Debug ] - source_sum: [$source_sum]\n" if $conf->{sys}{debug};
			}
		}
		close $file_handle;
		
		# If we got a sum, make sure it matches an expected one.
		if ($source_sum)
		{
			foreach my $this_sum (sort {$a cmp $b} keys %{$conf->{sys}{md5sum}})
			{
				print "[ Debug ] - source_sum: [$source_sum]\n" if $conf->{sys}{debug};
				print "[ Debug ] - this_sum:   [$this_sum]\n" if $conf->{sys}{debug};
				if ($source_sum eq $this_sum)
				{
					# Match!
					$conf->{sys}{os} = $conf->{sys}{md5sum}{$this_sum};
					print "[ Debug ] - sys::os: [$conf->{sys}{os}]\n" if $conf->{sys}{debug};
				}
			}
		}
		else
		{
			print "\n[ Error ] - Failed to calculate (or parse) the 'md5sum' for: [$conf->{switches}{source}].\n";
			print "[ Error ] - It is important to verify the source ISO, so unfortunately, we can not proceed.\n\n";
			exit(1);
		}
		
		# Did I find an OS?
		if ($conf->{sys}{os} eq "rhel")
		{
			print "- The source ISO is Red Hat Enterprise Linux v6.7.\n";
		}
		elsif ($conf->{sys}{os} eq "centos")
		{
			print "- The source ISO is CentOS v6.7.\n";
		}
		else
		{
			print "\n[ Error ] - Failed to match the source ISO's 'md5sum': [$source_sum] to a known sum.\n";
			print "[ Error ] - It is important to verify the source ISO, so unfortunately, we can not proceed.\n";
			print "[ NOTE ]  - The source MUST be v6.7 and it must be DVD disc 1.\n\n";
			exit(1);
		}
	}
	else
	{
		print "\n[ Error ] - No source ISO provided!\n";
		print "[ Note ]  - Run './$THIS_FILE -h' for usage.\n\n";
		exit(1);
	}
	
	return(0);
}

# This searches the PATH environment variable looking for the location of any executable not pre-defined.
sub find_executables
{
	my ($conf) = @_;
	print "[ Debug ] - Searching for executables...\n" if $conf->{sys}{debug};
	
	my $ok = 1;
	foreach my $executable (sort {$a cmp $b} keys %{$conf->{executable}})
	{
		next if $conf->{executable}{$executable} =~ /^\//;
		print "[ Debug ]   - Program: [$executable]... \n" if $conf->{sys}{debug};
		foreach my $path (split/:/, $ENV{'PATH'})
		{
			my $test_path = "$path/$executable";
			if ((-e $test_path) && (-x $test_path))
			{
				# Found it.
				$conf->{executable}{$executable} = $test_path;
				print "[ Debug ]     - Found it: [$conf->{executable}{$executable}].\n" if $conf->{sys}{debug};
				last;
			}
		}
		
		# Die if we didn't find it.
		if ($conf->{executable}{$executable} !~ /^\//)
		{
			$ok = 0;
			print "\n[ Error ] - Failed to find the: [$executable] program. Is it installed?\n";
			print "[ Error ] - If so, is it in one of the directories in your PATH environment variable?\n\n";
		}
	}
	exit(1) if not $ok;
	
	return(0);
}

